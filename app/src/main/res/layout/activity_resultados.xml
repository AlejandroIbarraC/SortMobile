<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Resultados" tools:layout_editor_absoluteY="25dp">

    <TextView
            android:text="@string/insertion_sort"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView5"
            android:textSize="24dp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="16dp" app:layout_constraintEnd_toEndOf="parent"/>
    <Button
            android:text="generar lista"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button4"
            android:onClick="generarListaI"
            app:layout_constraintStart_toStartOf="@+id/textView5" android:layout_marginTop="28dp"
            android:layout_marginStart="8dp"
            app:layout_constraintTop_toBottomOf="@+id/textView5"/>
    <TextView
            android:layout_width="wrap_content"
            android:textSize="19dp"
            android:layout_height="wrap_content"
            android:id="@+id/textView6"
            app:layout_constraintTop_toTopOf="@+id/button4" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="72dp" android:layout_marginStart="52dp"
            android:layout_marginBottom="70dp" app:layout_constraintBottom_toTopOf="@+id/button5"
            app:layout_constraintVertical_bias="1.0"/>
    <Button
            android:text="Ordenar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button5"
            android:onClick="ordenarInsertion"
            app:layout_constraintStart_toStartOf="@+id/button4"
            android:layout_marginTop="120dp" android:layout_marginStart="20dp"
            app:layout_constraintTop_toBottomOf="@+id/button4"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView7"
            android:textSize="20dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/button5" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView8"
            android:textSize="18dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginStart="16dp"
            android:layout_marginBottom="27dp"/>
    <Button
            android:text="S.S."
            android:layout_width="53dp"
            android:layout_height="wrap_content"
            android:id="@+id/button6"
            style="@style/Widget.AppCompat.Button.Colored"
            android:onClick="irSelection"
            app:layout_constraintBottom_toBottomOf="parent" android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginBottom="16dp"/>
</android.support.constraint.ConstraintLayout>